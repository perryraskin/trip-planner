// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  email     String
}

model Trips {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  nickname  String
  dateStart DateTime
  dateEnd   DateTime
}
